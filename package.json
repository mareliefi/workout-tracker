{
  "name": "workout-tracker",
  "version": "1.0.0",
  "description": "Full-stack workout tracker with Flask and React",
  "scripts": {
    "dev": "concurrently \"npm:server\" \"npm:client\"",
    "server": "cd app && flask run --host=0.0.0.0",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install-all": "pip install -r requirements.txt && npm install && cd frontend && npm install",
    "install-python": "pip install -r requirements.txt",
    "install-frontend": "cd frontend && npm install",
    "start": "npm run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "workout",
    "tracker",
    "flask",
    "react",
    "vite"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "recharts": "^3.5.1"
  }
}
