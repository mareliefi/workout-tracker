						{
							"listen": "test",
							"script": {
								"exec": [
									"// Extract token from cookie and save to environment",
									"const cookies = pm.cookies.toObject();",
									"if (cookies.jwt_token) {",
									"    pm.environment.set(\"auth_token\", cookies.jwt_token);",
									"    console.log(\"Token saved to environment variable\");",
