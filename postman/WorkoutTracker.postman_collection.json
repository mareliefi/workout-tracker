				{
					"name": "Create Workout Plan",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Check if workout plan creation was successful",
									"if (pm.response.code === 200) {",
									"    const responseData = pm.response.json();",
									"    ",
									"    // Save the workout plan ID to environment variable",
									"    if (responseData.workout_plan_id) {",
									"        pm.environment.set(\"workout_plan_id\", responseData.workout_plan_id);",
									"        console.log(\"✅ Workout Plan ID automatically saved:\", responseData.workout_plan_id);",
									"        console.log(\"You can now use {{workout_plan_id}} in other requests\");",
									"    } else {",
									"        console.log(\"❌ No workout_plan_id found in response\");",
									"        console.log(\"Response:\", responseData);",
									"    }",
									"} else {",
									"    console.log(\"❌ Workout plan creation failed with status:\", pm.response.code);",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
